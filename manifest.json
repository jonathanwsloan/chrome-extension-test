{
  "name": "Social Media Like Hider",
  "version": "1.0",
  "description": "Hide likes and profiles on social media",
  "content_scripts": [{
    "matches": [
      "https://twitter.com/*"
    ],
    "css": [ "toggle.css" ],
    "js": ["jquery-3.4.1.min.js", "content.js"]
  }],
  "permissions": [
    "webNavigation"
  ],
  "icons": {
    "16": "images/anonymous_icon16.png",
    "32": "images/anonymous_icon32.png",
    "48": "images/anonymous_icon48.png",
    "128": "images/anonymous_icon128.png"
  },
  "browser_action": {
    "default_icon": "images/anonymous_icon16.png"
  },
  "manifest_version": 2
}